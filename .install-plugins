fi

if [ ! -d "$ZSH_CUSTOM/plugins/pnpm" ]; then
  git clone git@github.com:ntnyq/omz-plugin-pnpm.git $ZSH_CUSTOM/plugins/pnpm
fi


if [ ! -d "$ZSH_CUSTOM/themes/powerlevel10k" ]; then
